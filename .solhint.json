{
  "extends": "solhint:default",
  "plugins": [],
    "rules": {
      "imports-on-top": "error",
      "use-forbidden-name": "error",
      "array-declaration-spaces": "error",
      "bracket-align": "error",
      "const-name-snakecase": "off",
      "avoid-suicide": "error",
      "avoid-sha3": "warn",
      "avoid-tx-origin:": "warn",
      "not-rely-on-time": "warn",
      "not-rely-on-block-hash": "warn"
    }
}
